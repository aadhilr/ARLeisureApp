    <title>@ViewBag.Title</title>

    <!-- Vendor CSS -->
    <link href="~/Content/Themes/material/vendors/bower_components/mediaelement/build/mediaelementplayer.css" rel="stylesheet">
    <link href="~/Content/Themes/material/vendors/bower_components/fullcalendar/dist/fullcalendar.min.css" rel="stylesheet">
    <link href="~/Content/Themes/material/vendors/bower_components/animate.css/animate.min.css" rel="stylesheet">
    <link href="~/Content/Themes/material/vendors/bower_components/bootstrap-sweetalert/lib/sweet-alert.css" rel="stylesheet">
