<body>

    <header id="header" class="clearfix" data-current-skin="teal">
        <ul class="header-inner">

            <li class="logo hidden-xs">
                <a href="index.html"></a>
            </li>

            <li class="pull-right">
                <a data-toggle="modal" href="#newUserRegister" class="btn bgm-black waves-effect">Create Account</a>
            </li>
        </ul>

    </header>

<section id="main">
    <section id="content">
        <div class="container">
            @RenderBody()
        </div>
    </section>
</section>

    <div class="modal fade" id="newUserRegister"  data-keyboard="false" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                @Html.Action("Register", "Login", new { Area = "" })
            </div>
        </div>
    </div>

<footer id="footer">
        Copyright &copy; 2016 IITS - CITY TOUR

