    <!-- Javascript Libraries -->
    <script src="~/Content/Themes/material/vendors/bower_components/jquery/dist/jquery.min.js"></script>
    <script src="~/Content/Themes/material/vendors/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>

    @RenderSection("scripts", required: false)

    <script src="~/Content/Themes/material/vendors/bower_components/flot/jquery.flot.js"></script>
    <script src="~/Content/Themes/material/vendors/bower_components/flot/jquery.flot.resize.js"></script>
    <script src="~/Content/Themes/material/vendors/bower_components/flot.curvedlines/curvedLines.js"></script>
    <script src="~/Content/Themes/material/vendors/sparklines/jquery.sparkline.min.js"></script>
    <script src="~/Content/Themes/material/vendors/bower_components/jquery.easy-pie-chart/dist/jquery.easypiechart.min.js"></script>

    <script src="~/Content/Themes/material/vendors/bower_components/moment/min/moment.min.js"></script>
    <script src="~/Content/Themes/material/vendors/bower_components/fullcalendar/dist/fullcalendar.min.js "></script>
    <script src="~/Content/Themes/material/vendors/bower_components/simpleWeather/jquery.simpleWeather.min.js"></script>
    <script src="~/Content/Themes/material/vendors/bower_components/Waves/dist/waves.min.js"></script>
    <script src="~/Content/Themes/material/vendors/bootstrap-growl/bootstrap-growl.min.js"></script>
    <script src="~/Content/Themes/material/vendors/bower_components/bootstrap-sweetalert/lib/sweet-alert.min.js"></script>
    <script src="~/Content/Themes/material/vendors/bower_components/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.concat.min.js"></script>

    <!-- Placeholder for IE9 -->
    <!--[if IE 9 ]>
        <script src="~/Content/Themes/material/vendors/bower_components/jquery-placeholder/jquery.placeholder.min.js"></script>
    <![endif]-->

    <script src="~/Content/Themes/material/js/flot-charts/curved-line-chart.js"></script>
    <script src="~/Content/Themes/material/js/flot-charts/line-chart.js"></script>
    <script src="~/Content/Themes/material/js/charts.js"></script>

    <script src="~/Content/Themes/material/js/charts.js"></script>
    <script src="~/Content/Themes/material/js/functions.js"></script>
    <script src="~/Content/Themes/material/js/demo.js"></script>
    <script src="~/Content/Themes/material/vendors/bower_components/bootstrap-sweetalert/lib/sweet-alert.min.js"></script>
    <script src="~/Content/Themes/material/vendors/bower_components/nouislider/distribute/jquery.nouislider.all.min.js"></script>
    <script src="~/Scripts/common_functions.js"></script>

    @if (@TempData["ErrorMeassage"] != null && @TempData["ErrorMeassage"].ToString() != "")
    {
        <script type='text/javascript'>
            $(document).ready(function () {
                ErrorMessage("@TempData["ErrorMeassage"].ToString()", "@TempData["ErrorMeassageContent"].ToString()");
            });
        </script>
    }

    @if (@TempData["SuccessMeassage"] != null && @TempData["SuccessMeassage"].ToString() != "")
    {
        <script type='text/javascript'>
            $(document).ready(function () {
                SuccessMessage("@TempData["SuccessMeassage"].ToString()", "@TempData["SuccessMeassageContent"].ToString()");
            });
        </script>
    }

</body>
</html>