<!DOCTYPE html>
<!--[if IE 9 ]><html class="ie9"><![endif]-->
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title</title>

    <!-- Vendor CSS -->
    <link href="~/Content/Themes/material/vendors/bower_components/fullcalendar/dist/fullcalendar.min.css" rel="stylesheet">
    <link href="~/Content/Themes/material/vendors/bower_components/animate.css/animate.min.css" rel="stylesheet">
    <link href="~/Content/Themes/material/vendors/bower_components/bootstrap-sweetalert/lib/sweet-alert.css" rel="stylesheet">
    <link href="~/Content/Themes/material/vendors/bower_components/material-design-iconic-font/dist/css/material-design-iconic-font.min.css" rel="stylesheet">
    <link href="~/Content/Themes/material/vendors/bower_components/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.min.css" rel="stylesheet">

    <link href="~/Content/Themes/material/vendors/bower_components/nouislider/distribute/jquery.nouislider.min.css" rel="stylesheet">

    <!-- CSS -->
    <link href="~/Content/Themes/material/css/app.min.1.css" rel="stylesheet">
    <link href="~/Content/Themes/material/css/app.min.2.css" rel="stylesheet">

    <link href="~/Content/Site.css" rel="stylesheet"/>
    @*<link href="~/Content/Themes/material/vendors/bootgrid/jquery.bootgrid.min.css" rel="stylesheet">*@
    @*<link href="~/Content/Themes/GlassTheme/css/bootstrap.min.css" rel="stylesheet" />*@

    <link href="~/Content/Themes/GlassTheme/css/smartadmin-skins.min.css" rel="stylesheet" />
    @*<link href="~/Content/Themes/GlassTheme/css/smartadmin-production.min.css" rel="stylesheet" />*@

    @*<script src="http://www.google.com/uds/api?file=uds.js&v=1.0&key=AIzaSyB6GEhgVHs50QnhOvR3wyCBAWjg9cJTIaw" type="text/javascript">
